{
    "name": "LM Evaluation Harness QR Code Benchmark",
    "image": "mcr.microsoft.com/devcontainers/python:1-bullseye",
    "features": {
        "ghcr.io/devcontainers/features/python:1": {
            "installTools": true
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance"
            ]
        }
    },
    "postCreateCommand": "pip install -e . && pip install wandb openai openrouter cairosvg pyzbar pillow"
}
